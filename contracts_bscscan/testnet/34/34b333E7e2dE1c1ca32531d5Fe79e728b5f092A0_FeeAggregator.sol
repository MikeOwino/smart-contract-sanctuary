contract FeeAggregator is Ownable {

    using SafeMath for uint256;
    using Address for address;

    uint256 private constant MAX = ~uint256(0);

    address[] public tokens;
    address public safeWallet;

    IPancakeRouter02 public router;

    constructor(IPancakeRouter02 _router, address _safeWallet, address _cummies, address _busd) public {
        router = _router;
        safeWallet = _safeWallet;
        tokens.push(_cummies); // tokens[0] is always cummies(Main Token)
        tokens.push(_busd);
        approveTokens();
    }

    function setRouter(IPancakeRouter02 _newRouter) public onlyOwner {
        router = _newRouter;
        approveTokens();
    }

    function setCummies(address _newCummies) public onlyOwner {
        tokens[0] = _newCummies;
    }

    function setSafeWallet(address _safeWallet) public onlyOwner {
        safeWallet = _safeWallet;
    }

    function addToken(address token) public onlyOwner {
        tokens.push(token);
        approveTokens();
    }

    function approveTokens() internal {
        for (uint i = 0; i < tokens.length; i++) {
            IBEP20 token = IBEP20(tokens[i]);
            token.approve(address(router), MAX);
        }
    }

    function sendValue(address payable recipient, uint256 amount) internal {
        require(address(this).balance >= amount, "Address: insufficient balance");

        // solhint-disable-next-line avoid-low-level-calls, avoid-call-value
        (bool success, ) = recipient.call{ value: amount }("");
        require(success, "Address: unable to send value, recipient may have reverted");
    }

    function depositBNB() payable public returns(bool success){
        return true;
    }

    function withdraw() public onlyOwner {
        sendValue(payable(safeWallet), address(this).balance);
        for (uint i = 0; i < tokens.length; i++) {
            IBEP20 token = IBEP20(tokens[i]);
            uint tokenBalance = token.balanceOf(address(this));
            if (tokenBalance > 0) {
                token.transfer(safeWallet, tokenBalance);
            }
        }
    }

    function addLP() public {
        swapForBNB();
        uint bnbBalance = address(this).balance;
        uint swapAmount = bnbBalance.div(2);
        require(swapAmount > 0, "insufficient swap amount");

        address[] memory path = new address[](2);
        path[0] = router.WETH();
        path[1] = tokens[0];

        router.swapExactETHForTokensSupportingFeeOnTransferTokens{value: swapAmount}(0, path, address(this), block.timestamp + 300);
        uint cummiesBalance = IBEP20(tokens[0]).balanceOf(address(this));
        require(cummiesBalance > 0, 'No Cummies');

        router.addLiquidityETH(tokens[0], cummiesBalance, 0, 0, safeWallet, block.timestamp + 300);
    }

    function swapForBNB() internal {
        for (uint i = 0; i < tokens.length; i++) {
            IBEP20 token = IBEP20(tokens[i]);
            if (token.balanceOf(address(this)) > 0) {
                address[] memory path = new address[](2);
                path[0] = tokens[i];
                path[1] = router.WETH();
                router.swapExactTokensForETHSupportingFeeOnTransferTokens(token.balanceOf(address(this)), 0, path, address(this), block.timestamp + 300);
            }
        }
    }
}